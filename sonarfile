pipeline
{
agent any
stages
{
 stage('scm checkout')
{steps{git branch: 'main', url: 'https://github.com/rahulbandral/devops24.git'}}
stage('execute unit test framework')
{steps{withMaven(globalMavenSettingsConfig: '', jdk: 'java', maven: 'maven', mavenSettingsConfig: '', traceability: true) {
    sh 'mvn test'
} }}
stage('sonar analysis')
{steps{withSonarQubeEnv(credentialsId: 'sonar'installationName: 'sonar') {
    sh 'sonar:sonar'
} }}
stage('generate artifacts')
{steps{withMaven(globalMavenSettingsConfig: '', jdk: 'java', maven: 'maven', mavenSettingsConfig: '', traceability: true) {
    sh 'mvn package'
} }}



}}
