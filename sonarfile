pipeline
{
agent any
stages
{
 stage('scm checkout')
{steps{git branch: 'main', url: 'https://github.com/rahulbandral/devops24'}}
stage('execute unit test framework')
{steps { withMaven(globalMavenSettingsConfig: '', jdk: 'JAVA_HOME', maven: 'MAVEN_HOME', mavenSettingsConfig: '', traceability: true) { sh 'mvn test'
} }}

}}

